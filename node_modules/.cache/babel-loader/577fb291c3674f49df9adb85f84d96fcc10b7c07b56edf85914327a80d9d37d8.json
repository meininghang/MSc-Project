{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapGetters } from 'vuex';\nimport { Othent } from 'othent';\nexport default {\n  name: \"loginWallet\",\n  data() {\n    return {\n      walletDialogVisible: false\n    };\n  },\n  computed: {\n    ...mapGetters({\n      account: 'account/account'\n    })\n  },\n  methods: {\n    ...mapActions({\n      getAccount: 'account/getNewMetaMaskAccount'\n    }),\n    openDialog() {\n      this.walletDialogVisible = true;\n    },\n    /**\r\n     * The old version of getting the account, because it will call the public key,\r\n     * it is not useful when logging in, so only get the account information when logging in\r\n     * @returns {Promise<void>}\r\n     */\n    async connectWallet() {\n      const account = await this.getAccount();\n      if (account && account.length !== 0) {\n        this.$router.push({\n          name: 'Main'\n        });\n      } else {\n        this.$message.error('Account verification failed');\n      }\n    },\n    async openGoogle() {\n      const othent = await Othent({\n        API_ID: '64e59f795c581daf3c18f9e7be2ea917'\n      });\n      const response = await othent.ping();\n      console.log(response);\n    }\n  }\n};","map":{"version":3,"names":["mapActions","mapGetters","Othent","name","data","walletDialogVisible","computed","account","methods","getAccount","openDialog","connectWallet","length","$router","push","$message","error","openGoogle","othent","API_ID","response","ping","console","log"],"sources":["src/components/__dialog/loginWallet.vue"],"sourcesContent":["<template>\r\n  <div class=\"wallet-list-dialog__main\">\r\n    <el-dialog\r\n        title=\"Connect Wallet\"\r\n        :visible.sync=\"walletDialogVisible\"\r\n        width=\"430px\"\r\n        center>\r\n      <div class=\"wallet-list-content\">\r\n        <div class=\"wallet-name-box\" @click=\"connectWallet\">\r\n          <img class=\"logo-style\" src=\"../../assets/img/MetaMask_Fox.svg.png\" alt=\"MetaMask\">\r\n          MetaMask\r\n        </div>\r\n        <div class=\"wallet-name-box\" @click=\"openGoogle\">\r\n          <img class=\"logo-style\" src=\"../../assets/img/MetaMask_Fox.svg.png\" alt=\"MetaMask\">\r\n          Othent\r\n        </div>\r\n      </div>\r\n      <p class=\"needing-attention\">By connecting your wallet you agree to <br/> the Terms of Service and Privacy Policy</p>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapActions, mapGetters} from 'vuex'\r\nimport { Othent } from 'othent'\r\n\r\nexport default {\r\n  name: \"loginWallet\",\r\n  data() {\r\n    return {\r\n      walletDialogVisible: false\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      account: 'account/account'\r\n    }),\r\n  },\r\n  methods: {\r\n    ...mapActions({\r\n      getAccount: 'account/getNewMetaMaskAccount'\r\n    }),\r\n    openDialog() {\r\n      this.walletDialogVisible = true\r\n    },\r\n    /**\r\n     * The old version of getting the account, because it will call the public key,\r\n     * it is not useful when logging in, so only get the account information when logging in\r\n     * @returns {Promise<void>}\r\n     */\r\n    async connectWallet() {\r\n      const account = await this.getAccount()\r\n      if (account && account.length !== 0) {\r\n        this.$router.push({\r\n          name: 'Main'\r\n        })\r\n      } else {\r\n        this.$message.error('Account verification failed')\r\n      }\r\n    },\r\n    async openGoogle() {\r\n      const othent = await Othent({\r\n        API_ID: '64e59f795c581daf3c18f9e7be2ea917'\r\n      })\r\n      const response = await othent.ping()\r\n      console.log(response)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.wallet-list-dialog__main {\r\n  .wallet-list-content {\r\n    width: 100%;\r\n  }\r\n\r\n  .wallet-name-box {\r\n    /*width: 320px;*/\r\n    width: 90%;\r\n    height: 48px;\r\n    border-radius: 5px;\r\n    background-color: #ececec;\r\n    margin: 0 auto;\r\n    text-align: center;\r\n    color: #222326;\r\n    font-size: 15px;\r\n    line-height: 48px;\r\n    box-shadow: 8px 8px 0px 0px rgba(0, 0, 0, .3);\r\n    cursor: pointer;\r\n    position: relative;\r\n\r\n    .logo-style {\r\n      width: 30px;\r\n      height: 30px;\r\n      position: absolute;\r\n      top: 8px;\r\n      left: 20px;\r\n    }\r\n  }\r\n\r\n  .wallet-name-box:hover {\r\n    box-shadow: 8px 8px 0px 0px rgba(0, 0, 0, 1);\r\n  }\r\n\r\n  .wallet-name-box:nth-child(2) {\r\n    margin: 20px auto;\r\n  }\r\n\r\n  .needing-attention {\r\n    width: 260px;\r\n    margin: 60px auto 30px;\r\n    text-align: center;\r\n    color: #acacac;\r\n  }\r\n}\r\n\r\n</style>"],"mappings":";AAuBA,SAAAA,UAAA,EAAAC,UAAA;AACA,SAAAC,MAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,mBAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAL,UAAA;MACAM,OAAA;IACA;EACA;EACAC,OAAA;IACA,GAAAR,UAAA;MACAS,UAAA;IACA;IACAC,WAAA;MACA,KAAAL,mBAAA;IACA;IACA;AACA;AACA;AACA;AACA;IACA,MAAAM,cAAA;MACA,MAAAJ,OAAA,cAAAE,UAAA;MACA,IAAAF,OAAA,IAAAA,OAAA,CAAAK,MAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;UACAX,IAAA;QACA;MACA;QACA,KAAAY,QAAA,CAAAC,KAAA;MACA;IACA;IACA,MAAAC,WAAA;MACA,MAAAC,MAAA,SAAAhB,MAAA;QACAiB,MAAA;MACA;MACA,MAAAC,QAAA,SAAAF,MAAA,CAAAG,IAAA;MACAC,OAAA,CAAAC,GAAA,CAAAH,QAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}