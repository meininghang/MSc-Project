{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getFileSize } from '@/utils/util';\nimport fileIconView from \"@/components/commonComponent/fileIconView\";\nexport default {\n  name: \"uploadBox\",\n  components: {\n    fileIconView\n  },\n  props: {\n    fileInfo: {\n      type: Object,\n      default: () => {}\n    }\n  },\n  data() {\n    return {\n      getFileSize,\n      uploadDialogShow: true,\n      inputTag: '',\n      changeName: this.fileInfo.file.name,\n      tagList: [],\n      showLoading: false,\n      uploadText: 'UPLOAD'\n    };\n  },\n  computed: {\n    fileSize() {\n      return getFileSize(this.fileInfo.file.size);\n    }\n  },\n  methods: {\n    openDialog() {\n      this.uploadDialogShow = true;\n    },\n    addTag() {\n      const obj = {\n        name: this.inputTag,\n        value: this.inputTag\n      };\n      this.tagList.push(obj);\n      this.inputTag = '';\n    },\n    closeTag(item) {\n      this.tagList = this.tagList.filter(value => value.name !== item.name);\n    },\n    uploadTagAndFile() {\n      this.$emit('uploadTagAndFile', this.tagList);\n      this.uploadText = 'UPLOADING...';\n      this.showLoading = true;\n    },\n    cancelUpload() {\n      this.showLoading = false;\n      this.$emit('cancelUploadBox');\n    },\n    changeFileName() {\n      const fileName = this.changeName;\n      this.$emit('changeName', fileName);\n    }\n  }\n};","map":{"version":3,"names":["getFileSize","fileIconView","name","components","props","fileInfo","type","Object","default","data","uploadDialogShow","inputTag","changeName","file","tagList","showLoading","uploadText","computed","fileSize","size","methods","openDialog","addTag","obj","value","push","closeTag","item","filter","uploadTagAndFile","$emit","cancelUpload","changeFileName","fileName"],"sources":["src/components/__dialog/uploadFileDialog.vue"],"sourcesContent":["<template>\r\n  <div class=\"upload-file__main\">\r\n    <el-dialog\r\n        title=\"Upload File\"\r\n        :visible.sync=\"uploadDialogShow\"\r\n        width=\"530px\"\r\n        center>\r\n      <div class=\"upload-style\">\r\n        <div class=\"file-size-show\">\r\n          Size: {{ fileSize }}\r\n        </div>\r\n        <div class=\"upload-box-file-name\">\r\n          <p class=\"span-table-text\">\r\n            <fileIconView :file-type=\"fileInfo.file.type\"></fileIconView>\r\n            <el-input\r\n                v-model=\"changeName\"\r\n                style=\"width: 100%\"\r\n                @keyup.enter.native=\"changeFileName\"\r\n                @change=\"changeFileName\"\r\n                class=\"upload-input-tag-list\">\r\n            </el-input>\r\n          </p>\r\n        </div>\r\n        <div class=\"upload-input-tag-list\">\r\n          <span style=\"width: 100px\">Tags:</span>\r\n          <el-input\r\n              v-model=\"inputTag\"\r\n              style=\"width: 100%\"\r\n              placeholder=\"Enter a keyword, ENTER next\"\r\n              @keyup.enter.native=\"addTag\"\r\n              class=\"upload-box-input-tag-list\">\r\n          </el-input>\r\n        </div>\r\n      </div>\r\n      <div class=\"tags-list-style\">\r\n        <p>\r\n          <el-tag\r\n              v-for=\"(item, index) in tagList\"\r\n              :key=\"index.name\"\r\n              type=\"info\"\r\n              closable\r\n              style=\"margin: 0 4px;\"\r\n              @close=\"closeTag(item)\">{{ item.name }}\r\n          </el-tag>\r\n        </p>\r\n      </div>\r\n      <div class=\"upload-file-upload-button\">\r\n        <p class=\"normal-upload-btn upload-box-cancel\" @click=\"cancelUpload\">\r\n          CANCEL\r\n        </p>\r\n        <p class=\"normal-upload-btn upload-box-btn\" @click=\"uploadTagAndFile\">\r\n          <i v-if=\"showLoading\" class=\"el-icon-loading\"></i>\r\n          {{ uploadText }}\r\n        </p>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {getFileSize} from '@/utils/util'\r\nimport fileIconView from \"@/components/commonComponent/fileIconView\"\r\n\r\nexport default {\r\n  name: \"uploadBox\",\r\n  components: {\r\n    fileIconView\r\n  },\r\n  props: {\r\n    fileInfo: {\r\n      type: Object,\r\n      default: () => {\r\n      }\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      getFileSize,\r\n      uploadDialogShow: true,\r\n      inputTag: '',\r\n      changeName: this.fileInfo.file.name,\r\n      tagList: [],\r\n      showLoading: false,\r\n      uploadText: 'UPLOAD'\r\n    }\r\n  },\r\n  computed: {\r\n    fileSize() {\r\n      return getFileSize(this.fileInfo.file.size)\r\n    }\r\n  },\r\n  methods: {\r\n    openDialog() {\r\n      this.uploadDialogShow = true\r\n    },\r\n    addTag() {\r\n      const obj = {\r\n        name: this.inputTag,\r\n        value: this.inputTag\r\n      }\r\n      this.tagList.push(obj)\r\n      this.inputTag = ''\r\n    },\r\n    closeTag(item) {\r\n      this.tagList = this.tagList.filter(value => value.name !== item.name)\r\n    },\r\n    uploadTagAndFile() {\r\n      this.$emit('uploadTagAndFile', this.tagList)\r\n      this.uploadText = 'UPLOADING...'\r\n      this.showLoading = true\r\n    },\r\n    cancelUpload() {\r\n      this.showLoading = false\r\n      this.$emit('cancelUploadBox')\r\n    },\r\n    changeFileName() {\r\n      const fileName = this.changeName\r\n      this.$emit('changeName', fileName)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.upload-file__main {\r\n  .upload-style {\r\n    width: 480px;\r\n    display: flex;\r\n    justify-content: start;\r\n    align-items: center;\r\n    flex-wrap: wrap;\r\n    flex-direction: column;\r\n  }\r\n\r\n  /*width: 778px;*/\r\n  .tags-list-style {\r\n    height: 35px;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    align-items: center;\r\n  }\r\n\r\n  .file-size-show {\r\n    font-size: 14px;\r\n    color: #09244B;\r\n    height: 50px;\r\n    line-height: 50px;\r\n    text-align: left;\r\n  }\r\n\r\n  .upload-file-upload-button {\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    align-items: center;\r\n    margin: 8px 0;\r\n  }\r\n\r\n  .normal-upload-btn {\r\n    width: 102px;\r\n    height: 28px;\r\n    line-height: 28px;\r\n    text-align: center;\r\n    font-size: 10px;\r\n    font-weight: bold;\r\n    border-radius: 10px;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .upload-box-file-name {\r\n    /*width: 300px;*/\r\n    white-space: nowrap;\r\n    overflow: hidden;\r\n    text-overflow: ellipsis;\r\n  }\r\n\r\n  .span-table-text {\r\n    display: flex;\r\n    align-items: center;\r\n    font-size: 14px;\r\n    color: #09244B;\r\n    height: 50px;\r\n  }\r\n\r\n  .upload-input-tag-list {\r\n    /*width: 100%;*/\r\n    display: flex;\r\n    justify-content: left;\r\n    align-items: center;\r\n  }\r\n\r\n  .upload-input-tag-list .el-input__inner {\r\n    /*width: 278px;*/\r\n    border: none;\r\n    background: none;\r\n    border-bottom: 2px solid #FAF9F8;\r\n  }\r\n\r\n  .upload-box-cancel {\r\n    border: 2px solid #FAF9F8;\r\n    background: #fbfbfb;\r\n    color: #A9A8B0;\r\n  }\r\n\r\n  .upload-box-btn {\r\n    border: 2px solid #6447F2;\r\n    background: #715EFF;\r\n    color: #ffffff;\r\n    margin-left: 30px;\r\n  }\r\n}\r\n</style>"],"mappings":";AA4DA,SAAAA,WAAA;AACA,OAAAC,YAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACAC,QAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,OAAA,EAAAA,CAAA,MACA;IACA;EACA;EACAC,KAAA;IACA;MACAT,WAAA;MACAU,gBAAA;MACAC,QAAA;MACAC,UAAA,OAAAP,QAAA,CAAAQ,IAAA,CAAAX,IAAA;MACAY,OAAA;MACAC,WAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACAC,SAAA;MACA,OAAAlB,WAAA,MAAAK,QAAA,CAAAQ,IAAA,CAAAM,IAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAX,gBAAA;IACA;IACAY,OAAA;MACA,MAAAC,GAAA;QACArB,IAAA,OAAAS,QAAA;QACAa,KAAA,OAAAb;MACA;MACA,KAAAG,OAAA,CAAAW,IAAA,CAAAF,GAAA;MACA,KAAAZ,QAAA;IACA;IACAe,SAAAC,IAAA;MACA,KAAAb,OAAA,QAAAA,OAAA,CAAAc,MAAA,CAAAJ,KAAA,IAAAA,KAAA,CAAAtB,IAAA,KAAAyB,IAAA,CAAAzB,IAAA;IACA;IACA2B,iBA