ntent: center;align-items: center;flex-wrap: wrap;\r\n      flex-direction: column;\">\r\n        <div style=\"font-size: 14px;color: #09244B;height: 50px;line-height: 50px;text-align: left\">\r\n          Size: {{getFileSize(fileInfo.file.size)}}\r\n        </div>\r\n        <div class=\"upload-box-file-name\">\r\n          <p\r\n              class=\"span-table-text\"\r\n              style=\"display: flex;align-items: center\">\r\n            <fileIconView :file-type=\"fileInfo.file.type\"></fileIconView>\r\n            <el-input\r\n                v-model=\"changeName\"\r\n                style=\"width: 100%\"\r\n                @keyup.enter.native=\"changeFileName\"\r\n                @change=\"changeFileName\"\r\n                class=\"upload-input-tag-list\">\r\n            </el-input>\r\n          </p>\r\n        </div>\r\n        <div class=\"upload-input-tag-list\" style=\"display: flex;justify-content: left;align-items: center\">\r\n          Tag:\r\n          <el-input\r\n              v-model=\"inputTag\"\r\n              style=\"width: 100%\"\r\n              placeholder=\"Enter a keyword, ENTER next\"\r\n              @keyup.enter.native=\"addTag\"\r\n              class=\"upload-box-input-tag-list\">\r\n          </el-input>\r\n        </div>\r\n      </div>\r\n      <div style=\"height:35px;display: flex;justify-content: flex-end;align-items: center\">\r\n        <p>\r\n          <el-tag\r\n              v-for=\"(item, index) in tagList\"\r\n              :key=\"index.name\"\r\n              type=\"info\"\r\n              closable\r\n              style=\"margin: 0 4px;\"\r\n              @close=\"closeTag(item)\">{{ item.name }}</el-tag>\r\n        </p>\r\n      </div>\r\n      <div class=\"upload-file-upload-button\" style=\"display: flex;justify-content: flex-end;align-items: center;margin: 8px 0\">\r\n        <p class=\"normal-upload-btn upload-box-cancel\" @click=\"cancelUpload\">\r\n          CANCEL\r\n        </p>\r\n        <p class=\"norma